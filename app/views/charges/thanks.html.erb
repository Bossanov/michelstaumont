<body>

  <i class="bi bi-list mobile-nav-toggle d-xl-none"></i>
  <%= render 'shared/navbar'%>
  <main id="main">
    <section class="breadcrumbs">
      <div class="container">
        <div class="d-flex justify-content-between align-items-center">
          <h2>Ma commande</h2>
          <ol>
            <li><a href="<%=root_path%>">Accueil</a></li>
            <li>Paiement</li>
          </ol>
        </div>
      </div>
    </section>
    <section class="inner-page">
      <div class="container">
        <h2>Vous avez payé ! Vous pouvez consulter votre compte-client pour suivre votre commande ! Merci !</h2>
        <%= link_to "Retour à l'accueil", root_path, class:"au-btn au-btn-icon au-btn--green au-btn--small"%>
        <%= link_to "Voir ma commande", root_path, class:"au-btn au-btn-icon au-btn--green au-btn--small"%>
      </div>
    </section>
  </main>
